package pl.edu.ur.oopl9;
import java.util.Arrays;
import java.util.Random;

public class zad1 {
    public static void main(String[] args){
    Random rand = new Random(); 
    
    int[] x = new int[100];
    for(int i=0; i<100;i++)
    {    
    
    x[i]=rand.nextInt(100); 
    
    
    }
    
    Arrays.sort(x);
    
    
    
    for(int i=0;i<x.length;i++){
        System.out.println(i+"="+x[i]);
                
}}}
